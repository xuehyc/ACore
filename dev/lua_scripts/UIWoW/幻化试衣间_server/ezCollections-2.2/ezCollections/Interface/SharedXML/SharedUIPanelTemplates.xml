	</Button>

	<Button name="UIMenuButtonStretchTemplate" virtual="true">
		<Size x="40" y="26"/>
		<Layers>
			<Layer level="BACKGROUND">
				<Texture parentKey="TopLeft" name="$parentTopLeft" file="Interface\AddOns\ezCollections\Interface\Buttons\UI-Silver-Button-Up">
					<Size x="12" y="6"/>
					<Anchors>
						<Anchor point="TOPLEFT"/>
					</Anchors>
